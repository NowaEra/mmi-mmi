{
    "name": "mmi/mmi",
    "type": "library",
    "description": "MMi Framework Library",
    "keywords": ["mmi", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mariusz Miłejko",
            "email": "mariusz@milejko.pl"
        },
        {
            "name": "Bartłomiej Wołos",
            "email": "funfel@onet.pl"
        },
        {
            "name": "Maciej Pawłowski",
            "email": "mac@macu.eu"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "monolog/monolog": "1.*",
        "graylog2/gelf-php": "1.5.*",
        "nowaera/mmi-core": "dev-master@dev"
    },
    "require-dev": {
        "php-console/php-console": "~3.1",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Mmi\\": "src/Mmi",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmi\\": "tests/Mmi"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.7-dev"
        }
    }
}
